{
  "$schema": "https://typedoc-plugin-markdown.org/schema.json",
  "name": "remark-example",
  "entryPoints": ["./src/index.ts"],
  "plugin": ["typedoc-plugin-markdown", "typedoc-plugin-remark"],
  "githubPages": false,
  "hidePageHeader": true,
  "readme": "none",
  "outputFileStrategy": "modules",
  "remarkPlugins": [
    "unified-prettier",
    [
      "remark-github",
      { "repository": "https://github.com/typedoc2md/typedoc-plugin-markdown" }
    ],
    ["remark-toc", { "heading": "Table of Contents", "maxDepth": 3 }]
  ],
  "gitRevision": "main"
}
